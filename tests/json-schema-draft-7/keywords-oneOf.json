{
  "$schema": "../../test-schema.json",
  "registry": {
    "http://example.com/": {
      "oneOf": [
        { "$id": "http://example.com/0", "title": "First!" },
        { "$id": "http://example.com/1", "title": "Second!" }
      ]
    }
  },
  "tests": [
    {
      "base_uri": "http://example.com/",
      "ref": "http://example.com/0",
